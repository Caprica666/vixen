<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Label="UserMacros" Condition="'$(Configuration)'=='Debug'">
    <BuildTargetSuffix>$(PlatformArchitecture)d</BuildTargetSuffix>
    <DebugSuffix>d</DebugSuffix>
  </PropertyGroup>
  <PropertyGroup Label="UserMacros" Condition="'$(Configuration)'=='Safe'">
    <BuildTargetSuffix>$(PlatformArchitecture)s</BuildTargetSuffix>
    <DebugSuffix>s</DebugSuffix>
  </PropertyGroup>
  <PropertyGroup Label="UserMacros" Condition="'$(Configuration)'=='Release'">
    <BuildTargetSuffix>$(PlatformArchitecture)</BuildTargetSuffix>
    <DebugSuffix>
    </DebugSuffix>
  </PropertyGroup>
  <PropertyGroup>
    <_ProjectFileVersion>10.0.30319.1</_ProjectFileVersion>
    <IntDir>$(VIXEN_DIR)\obj\$(TargetName)\</IntDir>
    <OutDir Condition="'$(TargetExt)'=='.lib'">$(VIXEN_DIR)\lib\</OutDir>
    <OutDir Condition="'$(TargetExt)'!='.lib'">$(VIXEN_DIR)\bin\$(Platform)$(DebugSuffix)\</OutDir>
    <TargetName>$(ProjectName)$(BuildTargetSuffix)</TargetName>
   </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <AdditionalIncludeDirectories>$(VIXEN_DIR)\inc;$(VIXEN_SDK)\dx11\inc;$(VIXEN_SDK)\freeimage\inc;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
      <PreprocessorDefinitions>STRICT;WIN32;_WINDOWS;VIXEN_DX11;VX_FREEIMAGE;_CRT_SECURE_NO_WARNINGS;%(PreprocessorDefinitions)</PreprocessorDefinitions>
      <WarningLevel>Level3</WarningLevel>
      <SuppressStartupBanner>true</SuppressStartupBanner>
      <PrecompiledHeaderFile>vixen.h</PrecompiledHeaderFile>
      <PrecompiledHeader>Use</PrecompiledHeader>
	   <OpenMPSupport>true</OpenMPSupport>
    </ClCompile>
    <Bscmake>
      <SuppressStartupBanner>true</SuppressStartupBanner>
      <OutputFile>$(IntDir)$(TargetName).bsc</OutputFile>
    </Bscmake>
    <Lib>
      <AdditionalLibraryDirectories>$(VIXEN_DIR)\lib;$(VIXEN_SDK)\dx11\lib\$(Platform);$(VIXEN_SDK)\freeimage\lib\$(Platform);%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
    </Lib>
    <Link>
      <SubSystem>Windows</SubSystem>
      <RandomizedBaseAddress>false</RandomizedBaseAddress>
      <SuppressStartupBanner>true</SuppressStartupBanner>
      <AdditionalLibraryDirectories>$(VIXEN_DIR)\lib;$(VIXEN_SDK)\dx11\lib\$(Platform);$(VIXEN_SDK)\freeimage\lib\$(Platform);%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
      <AdditionalDependencies>vixendx11$(BuildTargetSuffix).lib;d3d11.lib;d3dcompiler.lib;dxguid.lib;wininet.lib;urlmon.lib;winmm.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
    <ResourceCompile>
      <AdditionalIncludeDirectories>$(VIXEN_DIR)\inc;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
    </ResourceCompile>
  </ItemDefinitionGroup>
  <PropertyGroup Condition="'$(Configuration)'=='Safe'">
    <UseDebugLibraries>false</UseDebugLibraries>
  </PropertyGroup>
  <ItemDefinitionGroup Condition="'$(Configuration)'=='Safe'">
    <ClCompile>
      <Optimization>Full</Optimization>
      <InlineFunctionExpansion>AnySuitable</InlineFunctionExpansion>
      <FavorSizeOrSpeed>Speed</FavorSizeOrSpeed>
      <EnableFiberSafeOptimizations>true</EnableFiberSafeOptimizations>
      <PreprocessorDefinitions>STRICT;WIN32;_WINDOWS;VIXEN_DX11;_CRT_SECURE_NO_WARNINGS;_ASSERT;_SAFE;%(PreprocessorDefinitions)</PreprocessorDefinitions>
      <FloatingPointModel>Fast</FloatingPointModel>
      <FloatingPointExceptions>false</FloatingPointExceptions>
      <FunctionLevelLinking>true</FunctionLevelLinking>
      <BrowseInformation>true</BrowseInformation>
      <EnableEnhancedInstructionSet>StreamingSIMDExtensions2</EnableEnhancedInstructionSet>
      <OpenMPSupport>true</OpenMPSupport>
    </ClCompile>
    <Link>
      <GenerateDebugInformation>true</GenerateDebugInformation>
      <OptimizeReferences>true</OptimizeReferences>
      <EnableCOMDATFolding>true</EnableCOMDATFolding>
      <AssemblyDebug>true</AssemblyDebug>
    </Link>
    <Link Condition="'$(Platform)'!='x64'">
      <AdditionalDependencies>freeimage.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
    <Link Condition="'$(Platform)'=='x64'">
      <TargetMachine>MachineX64</TargetMachine>
      <AdditionalDependencies>freeimage.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
    <ProjectReference>
      <LinkLibraryDependencies>false</LinkLibraryDependencies>
    </ProjectReference>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup Condition="'$(Configuration)'=='Debug'">
    <Link Condition="'$(Platform)'!='x64'">
      <AdditionalDependencies>freeimage.lib;%(AdditionalDependencies)</AdditionalDependencies>
      <GenerateDebugInformation>true</GenerateDebugInformation>
     <AssemblyDebug>true</AssemblyDebug>
    </Link>
    <Link Condition="'$(Platform)'=='x64'">
      <AdditionalDependencies>freeimage.lib;%(AdditionalDependencies)</AdditionalDependencies>
      <TargetMachine>MachineX64</TargetMachine>
      <GenerateDebugInformation>true</GenerateDebugInformation>
    </Link>
   <ClCompile>
      <BrowseInformation>true</BrowseInformation>
      <Optimization>Disabled</Optimization>
      <RuntimeLibrary>MultiThreadedDebugDLL</RuntimeLibrary>
    </ClCompile>
  </ItemDefinitionGroup>
  <PropertyGroup Condition="'$(Configuration)'=='Release'">
    <UseDebugLibraries>false</UseDebugLibraries>
    <WholeProgramOptimization>true</WholeProgramOptimization>
  </PropertyGroup>
  <ItemDefinitionGroup Condition="'$(Configuration)'=='Release'">
    <ClCompile>
      <Optimization>Full</Optimization>
       <InlineFunctionExpansion>AnySuitable</InlineFunctionExpansion>
      <FavorSizeOrSpeed>Speed</FavorSizeOrSpeed>
      <EnableFiberSafeOptimizations>true</EnableFiberSafeOptimizations>
      <FloatingPointModel>Fast</FloatingPointModel>
      <FloatingPointExceptions>false</FloatingPointExceptions>
      <FunctionLevelLinking>true</FunctionLevelLinking>
      <RecognizeRestrictKeyword>true</RecognizeRestrictKeyword>
      <ExceptionHandling>Async</ExceptionHandling>
      <EnableEnhancedInstructionSet>StreamingSIMDExtensions2</EnableEnhancedInstructionSet>
      <OpenMPSupport>true</OpenMPSupport>
    </ClCompile>
    <Link>
      <OptimizeReferences>true</OptimizeReferences>
      <EnableCOMDATFolding>true</EnableCOMDATFolding>
      <LinkTimeCodeGeneration>UseLinkTimeCodeGeneration</LinkTimeCodeGeneration>
    </Link>
    <Link Condition="'$(Platform)'=='x64'">
      <TargetMachine>MachineX64</TargetMachine>
      <AdditionalDependencies>freeimage.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
    <Link Condition="'$(Platform)'!='x64'">
      <AdditionalDependencies>freeimage.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="BuildTargetSuffix">
      <Value>$(BuildTargetSuffix)</Value>
    </BuildMacro>
    <BuildMacro Include="VIXEN_SDK">
      <Value>$(VIXEN_SDK)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
  </ItemGroup>
</Project>